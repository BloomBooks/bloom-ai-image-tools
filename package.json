{
  "name": "bloom-ai-image-tools",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "assets",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "pnpm build:lib && pnpm build:demo",
    "build:demo": "vite build",
    "build:lib": "tsup",
    "test": "vitest run",
    "release": "pnpm build:lib && pnpm publish --access public",
    "changeset": "changeset",
    "preview": "vite preview",
    "//note that e2e tests require that you run build to pick up new code in the app (but not in the e2e tests themselves)": "",
    "e2e": "npx playwright test",
    "e2e:verbose": "cross-env E2E_VERBOSE=1 npx playwright test",
    "// This brings up the inspector and record button oh my...": "",
    "e2edebug": "echo To debug, right click on the e2e test and choose 'Debug Test'",
    "// This is the same as as e2edebug because neither actually runs a test": "",
    "e2erecord": "cross-env PWDEBUG=console  npx playwright test --debug"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "copy-image-clipboard": "^2.1.2",
    "idb-keyval": "^6.2.2",
    "json5": "^2.2.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.9",
    "@playwright/test": "^1.57.0",
    "@types/json5": "^2.2.0",
    "@types/node": "^25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "tsup": "^8.3.5",
    "typescript": "~5.8.2",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "volta": {
    "node": "24.12.0"
  }
}
